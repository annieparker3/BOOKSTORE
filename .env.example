# Server Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_APP_VERSION=1.0.0

# Authentication
VITE_SESSION_EXPIRY=86400 # 24 hours in seconds
VITE_RATE_LIMIT_WINDOW=15 # minutes
VITE_RATE_LIMIT_MAX=100 # max requests per window

# Security
VITE_CSRF_HEADER=X-CSRF-TOKEN
VITE_CSRF_COOKIE=csrf_token

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE=false
